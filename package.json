{
  "name": "mediapp",
  "version": "1.0.0",
  "description": "üè• MediApp - Healthcare Management System",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:docker-detached": "docker-compose -f docker-compose.dev.yml up --build -d",
    "prod": "docker-compose up --build",
    "prod:detached": "docker-compose up --build -d",
    "stop": "docker-compose down",
    "stop:dev": "docker-compose -f docker-compose.dev.yml down",
    "build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview",
    "lint": "cd frontend && npm run lint",
    "format": "cd frontend && npm run format",
    "typecheck": "cd frontend && npm run typecheck",
    "test": "echo \"No hay tests definidos\" && exit 0",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && go mod download"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "tailwindcss",
    "go",
    "postgresql"
  ],
  "author": "MediApp Team",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "msw": "^2.10.4",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
